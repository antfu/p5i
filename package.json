{
  "name": "p5i",
  "type": "module",
  "version": "0.4.2",
  "description": "p5.js, but with more friendly instance mode APIs",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/p5i#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/p5i.git"
  },
  "bugs": {
    "url": "https://github.com/antfu/p5i/issues"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/p5i.d.ts",
      "import": "./dist/p5i.mjs",
      "require": "./dist/p5i.cjs"
    }
  },
  "main": "dist/p5i.mjs",
  "module": "dist/p5i.mjs",
  "unpkg": "dist/p5i.browser.js",
  "jsdelivr": "dist/p5i.browser.js",
  "types": "dist/p5i.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "fetch": "esno scripts/fetch.ts",
    "watch": "npm run build -- --watch",
    "lint": "eslint .",
    "dev": "esno src/p5i.ts",
    "build": "rollup -c",
    "publish:ci": "npm publish --access public",
    "release": "npx bumpp --commit --push --tag && npm run publish:ci"
  },
  "dependencies": {
    "@types/p5": "^1.7.6",
    "p5": "^1.9.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.17.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/node": "^20.12.11",
    "axios": "^1.6.8",
    "eslint": "^9.2.0",
    "esno": "^4.7.0",
    "jsdom": "^24.0.0",
    "rollup": "^4.17.2",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.4.5"
  }
}
