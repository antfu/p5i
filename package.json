{
  "name": "p5i",
  "version": "0.0.1",
  "description": "p5.js, but with more friendly instance mode APIs",
  "main": "dist/p5i.js",
  "module": "dist/p5i.mjs",
  "types": "dist/p5i.d.ts",
  "jsdeliver": "dist/p5i.global.ts",
  "unpkg": "dist/p5i.global.ts",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/antfu/p5i/issues"
  },
  "homepage": "https://github.com/antfu/p5i#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/p5i.git"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "exports": "esno scripts/exports.ts",
    "watch": "npm run build -- --watch",
    "dev": "esno src/p5i.ts",
    "build": "rollup -c",
    "publish:ci": "npm publish --access public",
    "release": "npx bumpp --commit --push --tag && npm run publish:ci"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@types/p5": "^0.9.1"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.4.3",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@types/node": "^14.14.10",
    "eslint": "^7.14.0",
    "esno": "^0.3.0",
    "jsdom": "^16.4.0",
    "p5": "^1.1.9",
    "puppeteer": "^5.5.0",
    "rollup": "^2.34.0",
    "rollup-plugin-dts": "^2.0.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "typescript": "^4.1.2"
  }
}
